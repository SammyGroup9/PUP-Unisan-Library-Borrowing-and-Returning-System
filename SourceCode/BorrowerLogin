    void hideSomeSwing(){
        labelTryAgain.setVisible(false);
        labelCount.setVisible(false);
    }
    void viewSomeSwing(){
        labelTryAgain.setVisible(true);
        labelCount.setVisible(true);
    }
    void disableForm(){
        txtUsername.setText("Username");
        passUsername.setText("Password");
        passUsername.setEchoChar('\u0000');
        txtUsername.setEditable(false);
        passUsername.setEditable(false);
        showPassword.setEnabled(false);
        btnSubmit.setEnabled(false);
        btnClear.setEnabled(false);
        btnRegister.setEnabled(false);
        btnBack.setEnabled(false);
        btnSubmit.requestFocus();
        countdown();
    }
    
    void enableForm(){
        txtUsername.setEditable(true);
        passUsername.setEditable(true);
        showPassword.setEnabled(true);
        btnSubmit.setEnabled(true);
        btnClear.setEnabled(true);
        btnRegister.setEnabled(true);
        btnBack.setEnabled(true);

    }
    private void countdown(){
        count = 10;
        
        timer = new Timer();
        timerTask = new TimerTask() {
            @Override
            public void run() {
                if(count > 0){
                    viewSomeSwing();
                    int minutes = count / 60;
                    int seconds = count % 60;
                    labelCount.setText(minutes+":"+seconds);
//                    System.out.println(minutes+":"+seconds);
                    count --;
                }else{
                    timer.cancel();
                    hideSomeSwing();
                    enableForm();
                    loginAttempt = 0;
                }
            }
        };
        timer.scheduleAtFixedRate(timerTask,0,1000);
    }
    private Timer timer;
    private int count;
    private TimerTask timerTask;
    private int loginAttempt=0;
