    private void txtUsernameActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void showPasswordActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
        if(showPassword.isSelected()){
            passUsername.setEchoChar((char)0);
        }
        else{
            passUsername.setEchoChar('\u2022');

        }
    }                                            

    private void btnRegisterActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
        // direct to other frame
        setVisible(false);
        new BorrowerRegister().setVisible(true);
    }                                           

    private void btnBackActionPerformed(java.awt.event.ActionEvent evt) {                                        
        setVisible(false);
        new LoginAs().setVisible(true);
    }                                       

    private void btnSubmitActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        String username = txtUsername.getText();
        String password = passUsername.getText();
                // if else to check whether the textfield is empty
                if(username.equals("")){
                    JOptionPane.showMessageDialog(null,"Field is empty");
                }
                else if(username.equals("Username")){
                    JOptionPane.showMessageDialog(null,"Field is empty");
                }
                else if(password.equals("")){
                    JOptionPane.showMessageDialog(null,"Field is empty");    
                }
                else if(password.equals("Password")){
                    JOptionPane.showMessageDialog(null,"Field is empty");
                }else{
                     boolean isValid = DataHandler.loginUser(username, password);
                     if(isValid){
                        setVisible(false);
//                        new LibraryRoom().setVisible(true);                     
                     }else{
                         loginAttempt ++;
                         System.out.println(loginAttempt);
                        JOptionPane.showMessageDialog(null,"Incorrect username or password");
                        if(loginAttempt == 3){
                            disableForm();
                            JOptionPane.showMessageDialog(null,"Reached maximum attempts\n Login Failed","Login Failed",JOptionPane.ERROR_MESSAGE);
                        } 
                     }
                }
   


    }                                         

    private void btnClearActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        // To clear the textField
        txtUsername.setText("");
        passUsername.setText("");
        
    }                                        

    private void passUsernameFocusGained(java.awt.event.FocusEvent evt) {                                         
        // TODO add your handling code here:
        if(passUsername.getText().equals("Password")){
            passUsername.requestFocus();
            passUsername.setText("");
            passUsername.setEchoChar('\u2022');
        }
    }                                        

    private void passUsernameFocusLost(java.awt.event.FocusEvent evt) {                                       
        // TODO add your handling code here:
        if(passUsername.getText().length()==0){
            passUsername.setText("Password");
            passUsername.setEchoChar('\u0000');
        }
    }                                      

    private void txtUsernameFocusGained(java.awt.event.FocusEvent evt) {                                        
        // TODO add your handling code here:
        if(txtUsername.getText().equals("Username")){
            txtUsername.requestFocus();
            txtUsername.setText("");

        }
    }                                       

    private void txtUsernameFocusLost(java.awt.event.FocusEvent evt) {                                      
        // TODO add your handling code here:
        if(txtUsername.getText().length()==0){
            txtUsername.setText("Username");

        }
    }                                     

    private void passUsernameActionPerformed(java.awt.event.ActionEvent evt) {                                             
        // TODO add your handling code here:
    }                                            
    
   
    
    
