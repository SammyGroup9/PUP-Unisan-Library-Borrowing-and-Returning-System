package com.mycompany.librarymanagementsystem;

/**
 *
 * @author Errica
 */
public class checkAdmin {
        public static boolean checkInput(String lblUsername, String passUsername) {
            if(lblUsername.equals("PUP-Admin") && passUsername.equals("LibraryPassword")) {
                return true;
            }else {
                return false;
            }
        }
}
